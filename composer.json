{
  "name": "myonlinestore/gearmanmanager",
  "description": "PHP daemon for managing gearman workers",
  "keywords": ["gearman","net_gearman","gearman-manager"],
  "homepage": "https://github.com/MyOnlineStore/GearmanManager",
  "license": "BSD-2-Clause",
  "autoload": {
    "files": [
      "GearmanManager.php",
      "GearmanPearManager.php",
      "GearmanPeclManager.php",
      "pear-manager.php",
      "pecl-manager.php"
    ]
  },
  "require": {
    "php": ">=5.3.0"
  },
  "suggest": {
    "brianlmoon/net_gearman" : "A PHP interface for Gearman (requires version 1.0.*)",
    "ext-gearman" : "PHP wrapper to libgearman"
  },
  "bin": ["pear-manager.php", "pecl-manager.php"]
}
